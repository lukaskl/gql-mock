{
  "name": "graphql-codegen-types-map",
  "version": "1.0.0",
  "repository": "git@github.com:lukaskl/graphql-codegen-types-map.git",
  "author": "lukaskl",
  "license": "MIT",
  "sideEffects": false,
  "main": "dist/index.js",
  "typings": "dist/index.d.ts",
  "keywords": [
    "graphql-code-generator",
    "typescript",
    "node"
  ],
  "scripts": {
    "lint": "esw --color \"./src/**/*.{js,ts}\"",
    "lint:watch": "yarn lint --watch --changed --cache",
    "build": "tsc --project tsconfig.prod.json",
    "build:watch": "yarn build --watch",
    "dev": "run-p lint:watch build:watch",
    "test": "yarn jest",
    "test:watch": "yarn test --watch",
    "lint:ci": "yarn lint --max-warnings 0",
    "clean:ci": "rm -rf ./build",
    "pack:ci": "yarn lint:ci && yarn clean:ci && yarn build"
  },
  "dependencies": {
    "@graphql-codegen/plugin-helpers": "^1.13.1",
    "@graphql-codegen/testing": "^1.13.1",
    "@graphql-codegen/visitor-plugin-common": "^1.13.1",
    "change-case": "^4.1.1",
    "esprima": "^3.1.3",
    "graphql": "^14.6.0",
    "lazy-get-decorator": "^2.2.0",
    "lodash.merge": "^4.6.2",
    "lodash.uniq": "^4.5.0",
    "npm-run-all": "^4.1.5",
    "static-eval": "^2.0.5"
  },
  "devDependencies": {
    "@babel/core": "^7.9.0",
    "@babel/plugin-proposal-class-properties": "^7.7.0",
    "@babel/plugin-proposal-optional-chaining": "^7.9.0",
    "@babel/preset-env": "^7.7.1",
    "@babel/preset-typescript": "^7.7.2",
    "@types/esprima": "^2.1.35",
    "@types/express": "^4.17.2",
    "@types/jest": "^25.1.4",
    "@types/lodash.merge": "^4.6.6",
    "@types/lodash.uniq": "^4.5.6",
    "@types/static-eval": "^0.2.30",
    "@typescript-eslint/eslint-plugin": "^2.8.0",
    "@typescript-eslint/parser": "^2.8.0",
    "babel-jest": "^25.2.3",
    "eslint": "^6.6.0",
    "eslint-config-prettier": "^6.7.0",
    "eslint-plugin-jest": "^23.8.2",
    "eslint-plugin-jest-formatting": "^1.2.0",
    "eslint-plugin-prettier": "^3.1.2",
    "eslint-watch": "^5.0.1",
    "jest": "^25.2.3",
    "jest-runner-eslint": "^0.7.6",
    "prettier": "1.19.1",
    "ts-jest": "^25.2.1",
    "ts-node": "^8.8.1",
    "tsconfig-paths": "^3.9.0",
    "typescript": "^3.8.3"
  }
}
